<% if @friend_feeds.any? %>
  <h3>Friend Stories</h3>
  <ol class="stories">
    <%= render partial: 'shared/feed_item', collection: @friend_feeds %>
  </ol>
  <%= will_paginate @friend_feeds %>
<% end %>
